<Application x:Class="MyCommander.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MyCommander"
             xmlns:controls="clr-namespace:MyCommander.UserControls"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:AttributeConverter x:Key="attrConverter"/>
        <local:FileSystemInfoWrapperToImage x:Key="fSIWToImageConverter"/>

        <DataTemplate DataType="{x:Type controls:TabViewModel}">
            <controls:CommanderTab />
        </DataTemplate>
        
    </Application.Resources>
</Application>
