<Application x:Class="MyCommander.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MyCommander"
             xmlns:controls="clr-namespace:MyCommander.UserControls"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="DialogServices/DialogViewResourceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <local:AttributeConverter x:Key="attrConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
